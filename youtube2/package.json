{
	"name": "youtube2",
	"version": "1.2.2",
	"description": "YouTube plugin for Volumio",
	"main": "./dist/index.js",
	"scripts": {
		"build": "npm run prepare",
		"prepare": "rm -rf dist && npx tsc --build src && bash fixup.sh",
		"lint": "npx eslint ./src",
		"lint:fix": "npx eslint ./src --fix"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/patrickkfkan/volumio-youtube2.git"
	},
	"author": "Patrick Kan",
	"license": "MIT",
	"volumio_info": {
		"prettyName": "YouTube2",
		"plugin_type": "music_service",
		"os": [
			"buster"
		],
		"icon": "fa-youtube-play",
		"boot_priority": 10,
		"details": "YouTube plugin for Volumio",
		"changelog": "",
		"architectures": [
			"amd64",
			"armhf",
			"i386"
		]
	},
	"engines": {
		"node": ">=14",
		"volumio": ">=3"
	},
	"devDependencies": {
		"@types/fs-extra": "^8.1.2",
		"@types/md5": "^2.3.2",
		"@types/node": "^14.18.38",
		"@types/node-fetch": "^2.6.4",
		"@types/string-format": "^2.0.0",
		"@typescript-eslint/eslint-plugin": "^5.56.0",
		"@typescript-eslint/parser": "^5.56.0",
		"eslint": "^8.36.0",
		"ts-node": "^10.9.1",
		"typescript": "^4.9.5",
		"winston": "^3.8.2"
	},
	"dependencies": {
		"kew": "^0.7.0",
		"node-fetch": "^2.6.7",
		"now-playing-common": "git+https://github.com/patrickkfkan/volumio-now-playing-common#v0.3.2",
		"string-format": "^2.0.0",
		"v-conf": "^1.4.2",
		"volumio-youtubei.js": "git+https://github.com/patrickkfkan/Volumio-YouTube.js.git#v0.3.9"
	}
}
