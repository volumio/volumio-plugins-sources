{
  "page": {
    "label": "TRANSLATE.LMSDE_CONFIGURATION"
  },
  "sections": [
    {
      "id": "section_info",
      "element": "section",
      "label": "TRANSLATE.LMSDE_INFO",
      "icon": "fa-server",
      "content": [
        {
          "id": "url",
          "type": "text",
          "element": "input",
          "label": "TRANSLATE.LMSDE_URL"
        },
        {
          "id": "openUrl",
          "element": "button",
          "label": "TRANSLATE.LMSDE_OPEN_URL",
          "onClick": {
              "type": "openUrl"
          }
        },
        {
          "id": "viewStats",
          "element": "button",
          "label": "TRANSLATE.LMSDE_VIEW_STATS",
          "onClick": {
            "type": "emit",
            "message": "callMethod",
            "data": {
                "endpoint": "music_service/lmsde",
                "method": "viewStats"
            }
          }
        },
        {
          "id": "viewReadme",
          "element": "button",
          "label": "TRANSLATE.LMSDE_VIEW_README",
          "onClick": {
              "type": "openUrl",
              "url": "https://github.com/patrickkfkan/volumio-lmsde#readme"
          }
        }
      ]
    }
  ]
}
