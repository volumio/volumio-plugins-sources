{
  "name": "volumio-plugins-sources",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "format": "prettier --write '**/*.{json,md}' && npm run lint:es -- --fix",
    "lint": "npm run lint:es && npm run lint:prettier",
    "lint:es": "eslint .",
    "lint:prettier": "prettier --check '**/*.{json,md}'",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.23.9",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "prettier": "^3.2.4"
  }
}
